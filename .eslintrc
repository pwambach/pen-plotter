{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true
  },
  "extends": [
    "prettier"
  ],
  "rules": {
    "no-cond-assign": [2, "except-parens"],
    "no-console": [1, {"allow": ["warn", "error"]}],
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": 0,
    "no-func-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-prototype-builtins": 1,
    "no-regex-spaces": 1,
    "no-sparse-arrays": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-unsafe-finally": 2,
    "use-isnan": 2,
    "valid-jsdoc": [2, {
      "requireReturn": false,
      "requireReturnDescription": true,
      "requireParamDescription": true,
      "requireReturnType": true,
      "prefer": {
        "return": "return"
      },
      "preferType": {
        "Number": "number",
        "String": "string",
        "Boolean": "boolean"
      }
    }],
    "valid-typeof": 2,

    "accessor-pairs": 0,
    "array-callback-return": 2,
    "no-var": 2,
    "block-scoped-var": 2,
    "complexity": [1, 7],
    "consistent-return": 2,
    "curly": [2, "all"],
    "default-case": 0,
    "dot-notation": [1, {
      "allowKeywords": true
    }],
    "eqeqeq": [2, "smart"],
    "guard-for-in": 1,
    "no-alert": 2,
    "no-caller": 2,
    "no-case-declarations": 2,
    "no-div-regex": 1,
    "no-else-return": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-implicit-coercion": [2, {
      "boolean": true,
      "number": true,
      "string": true
    }],
    "no-implied-eval": 2,
    "no-invalid-this": 2,
    "no-iterator": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-process-env": 0,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-unused-expressions": 2,
    "no-useless-call": 2,
    "no-void": 2,
    "no-warning-comments": 1,
    "no-with": 2,
    "radix": 2,
    "vars-on-top": 0,
    "yoda": [2, "never"],

    "strict": [0, "never"],

    "init-declarations": [1, "always"],
    "no-catch-shadow": 2,
    "no-delete-var": 2,
    "no-label-var": 2,
    "no-shadow": 1,
    "no-shadow-restricted-names": 2,
    "no-undef": 2,
    "no-undef-init": 2,
    "no-undefined": 2,
    "no-unused-vars": [2, {
      "vars": "all",
      "args": "after-used"
    }],
    "no-use-before-define": [2, "nofunc"],

    "callback-return": 2,
    "handle-callback-err": 1,
    "no-mixed-requires": 0,
    "no-new-require": 2,
    "no-path-concat": 2,
    "no-process-exit": 1,
    "no-restricted-modules": 0,
    "no-sync": 0,
    "camelcase": 2,
    "consistent-this": [2, "none"],
    "func-names": 0,
    "func-style": 0,
    "id-length": [0],
    "id-match": [0],
    "max-nested-callbacks": [2, 3],
    "new-cap": 2,
    "no-array-constructor": 2,
    "no-inline-comments": 0,
    "no-lonely-if": 2,
    "no-nested-ternary": 2,
    "no-new-object": 2,
    "no-ternary": 0,
    "no-underscore-dangle": 1,
    "one-var": 0,
    "operator-assignment": 0,
    "quote-props": [0, "as-needed", {
      "keywords": false
    }],
    "quotes": [2, "single", "avoid-escape"],
    "sort-vars": 0,
    "spaced-comment": [2, "always"],
    "wrap-regex": 0,

    "max-depth": [1, 3],
    "max-len": 0,
    "max-params": [2, 4],
    "max-statements": [1, 15],
    "no-bitwise": 2,
    "no-plusplus": 0,
    "no-class-assign": 2,
    "no-const-assign": 2,
    "arrow-body-style": [1, "as-needed"],
    "constructor-super": 2,
    "no-confusing-arrow": 1,
    "no-dupe-class-members": 2,
    "no-duplicate-imports": 1,
    "no-new-symbol": 2,
    "no-this-before-super": 2,
    "no-useless-computed-key": 2,
    "no-useless-constructor": 2,
    "object-shorthand": 2,
    "prefer-arrow-callback": [1, {
      "allowUnboundThis": false
    }],
    "prefer-const": [2, {"destructuring": "all"}],
    "prefer-reflect": 0,
    "prefer-rest-params": 1,
    "prefer-spread": 1,
    "prefer-template": 0,
    "require-yield": 1,
    "sort-imports": 0
  }
}
